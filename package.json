{
  "name": "player",
  "version": "1.0.0",
  "description": "a simple music player",
  "scripts": {
    "start": "webpack-dev-server",
    "start:prod": "npm run build && serve dist",
    "build": "webpack --env.production",
    "analyze": "webpack --env.production --json | webpack-bundle-size-analyzer",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "format": "prettier-eslint --write '**/*.{js,scss}'",
    "precommit": "lint-staged",
    "deploy": "sh deploy"
  },
  "dist_dir": "dist",
  "browserslist": [
    "chrome > 51"
  ],
  "author": "dgeibi",
  "license": "MIT",
  "dependencies": {
    "antd": "^2.13.6",
    "anujs": "github:RubyLouvre/anu#7b80949",
    "babel-polyfill": "^7.0.0-beta.3",
    "fasy": "^1.0.1",
    "id3-parser": "^1.2.0",
    "idb": "^2.0.4",
    "is-promise": "^2.1.0",
    "nanoid": "^0.2.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-feather": "^1.0.7",
    "react-hot-loader": "^3.0.0",
    "uemitter": "0.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^7.0.0-beta.3",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-preset-react": "^7.0.0-beta.3",
    "babel-preset-stage-3": "^7.0.0-beta.3",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-config-dgeibi": "^3.0.6",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "2.30.1",
    "husky": "^0.14.3",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lint-staged": "^4.2.3",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^1.0.2",
    "precss": "^2.0.0",
    "prettier-eslint-cli": "^4.4.0",
    "script-ext-html-webpack-plugin": "^1.8.5",
    "serve": "^6.3.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.7.1",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0",
    "workbox-webpack-plugin": "^2.1.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 90
  },
  "lint-staged": {
    "*.{scss,js}": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
