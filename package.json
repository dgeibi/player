{
  "name": "player",
  "version": "1.0.0",
  "description": "a simple music player",
  "scripts": {
    "dev": "webpack-dev-server --config config/webpack.config.js",
    "dev:react": "webpack-dev-server --env.react --config config/webpack.config.js",
    "serve": "npm run build && serve dist",
    "build": "rimraf dist && webpack --env.production --config config/webpack.config.js",
    "analyze": "webpack --env.production --env.analyzer --config config/webpack.config.js",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,scss}\"",
    "precommit": "lint-staged",
    "deploy": "sh deploy"
  },
  "browserslist": [
    "chrome > 51"
  ],
  "author": "dgeibi",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.38",
    "antd": "^3.1.4",
    "anujs": "^1.2.4",
    "fasy": "^5.0.3",
    "id3-parser": "^2.0.0",
    "idb": "^2.0.4",
    "is-promise": "^2.1.0",
    "nanoid": "^1.0.1",
    "react-document-title": "^2.0.3",
    "react-feather": "^1.0.8",
    "uemitter": "0.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "@babel/preset-react": "^7.0.0-beta.38",
    "@babel/preset-stage-3": "^7.0.0-beta.38",
    "@easyops/git-exec-and-restage": "^1.0.4",
    "autoprefixer": "^7.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "prop-types": "^15.6.0",
    "babel-loader": "^8.0.0-beta.0",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.16.0",
    "eslint-config-dgeibi": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "2.30.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "postcss-loader": "^2.0.9",
    "postcss-scss": "^1.0.2",
    "precss": "^2.0.0",
    "prettier": "^1.9.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "serve": "^6.4.2",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1",
    "workbox-webpack-plugin": "^2.1.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 90
  },
  "lint-staged": {
    "*.{scss,js}": [
      "git-exec-and-restage prettier --write --"
    ]
  }
}
