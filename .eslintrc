{
  "extends": [
    "dgeibi/react"
  ],
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "class-methods-use-this": "off",
    "global-require": "off",
    "no-console": "off",
    "prefer-destructuring": "off"
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "config/webpack.base.js"
      }
    }
  }
}
