{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": "usage"
      }
    ],
    "@babel/preset-stage-3",
    "@babel/preset-react"
  ],
  "plugins": [
    "babel-plugin-transform-decorators-legacy",
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ]
}
